{
  "name": "dnu",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "precommit": [
    "precommit-msg",
    "test"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/from2": "^2.3.0",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/rmfr": "^2.0.0",
    "@types/supertest": "^2.0.4",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jest": "^23.1.0",
    "pre-commit": "^1.2.2",
    "regenerator-runtime": "^0.11.1",
    "rmfr": "^2.0.0",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "from2": "^2.3.0",
    "stream-concat": "^0.3.0",
    "streamifier": "^0.1.1"
  }
}