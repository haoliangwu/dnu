    }
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.0.0",
    "@types/supertest": "^2.0.4",
    "express": "^4.16.3",
    "jest": "^23.1.0",
    "regenerator-runtime": "^0.11.1",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  }
}
